<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!--Notice to Bygdrift developer: Create nuget package: dotnet pack-->
    <TargetFramework>net5.0</TargetFramework>
    <Version>0.3.7</Version>
    <Authors>kenboc</Authors>
    <Company>Bygdrift</Company>
    <Description>Create modules that can push data to a data warehouse in Azure, and retrieve data in Power BI through CDM model.</Description>
    <Copyright>https://github.com/Bygdrift/Warehouse/blob/master/License.md</Copyright>
    <PackageProjectUrl>https://github.com/Bygdrift/Warehouse</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/Bygdrift/Bygdrift.Warehouse.git</RepositoryUrl>
    <PackageTags>Datalake,CDM,Warehouse,Azure</PackageTags>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>Bygdrift.Warehouse</PackageId>
    <Product>Bygdrift.Warehouse</Product>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>It is no longer fixed, what to call the folders in the data warehouse.</PackageReleaseNotes>
    <AssemblyVersion>0.3.7.0</AssemblyVersion>
    <FileVersion>0.3.7.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Files.DataLake" Version="12.7.0" />
    <PackageReference Include="Bygdrift.CsvTools" Version="0.1.3" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />

    <!--For test-->
    <Reference Include="CsvTools"><HintPath>..\..\CsvTools\CsvTools\bin\Debug\net5.0\CsvTools.dll</HintPath></Reference>
    
  </ItemGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
