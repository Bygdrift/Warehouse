<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>0.1.2-beta</Version>
    <Authors>kenboc</Authors>
    <Company>Bygdrift</Company>
    <Description>Create modules that can push data to a data warehouse in Azure, and retrieve data in Power BI through CMD model.</Description>
    <Copyright>https://github.com/Bygdrift/Warehouse/blob/master/License.md</Copyright>
    <PackageProjectUrl>https://github.com/Bygdrift/Warehouse</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/Bygdrift/Bygdrift.Warehouse.git</RepositoryUrl>
    <PackageTags>Datalake,CMD,Warehouse,Azure</PackageTags>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>Bygdrift.Warehouse</PackageId>
    <Product>Bygdrift.Warehouse</Product>
    <PackageLicenseExpression>BSD-4-Clause</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Files.DataLake" Version="12.7.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
    <PackageReference Include="NCrontab.Signed" Version="3.3.2" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />    
  </ItemGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
